<Page x:Class="Мухутдинов.NewFolder1.HearderPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Мухутдинов.NewFolder1"
      mc:Ignorable="d" 
      d:DesignHeight="60" d:DesignWidth="800"
      Title="HearderPage">

    <Grid>
        <StackPanel  Orientation="Horizontal">
            <Image x:Name="HearderLogo" Source="/NewFolder1/RIS.png" Width="35" Height="31" Margin="10,5,15,5"/>
            <TextBlock VerticalAlignment="Center" FontSize="24" Margin="10,0,0,0" Name="HeaderText">Главное меню</TextBlock>
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Grid.Column="2" Grid.Row="0"/>
        </StackPanel>
    </Grid>
</Page>
